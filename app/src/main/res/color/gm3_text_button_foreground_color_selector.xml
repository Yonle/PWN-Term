<?xml version="1.0" encoding="utf-8" ?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_enabled="false"
		android:color="?attr/colorOnSurface"
		android:alpha="@dimen/material_emphasis_disabled" />
	<item
		android:state_checkable="false"
		android:state_pressed="true"
		android:color="?attr/colorOnContainerStateContent" />
	<item
		android:state_focused="true"
		android:state_checkable="false"
		android:color="?attr/colorOnContainerStateContent" />
	<item
		android:state_checkable="false"
		android:color="?attr/colorOnContainerStateContent"
		android:state_hovered="true" />
	<item
		android:state_checkable="false"
		android:color="?attr/colorOnContainer" />
	<item
		android:state_checked="true"
		android:state_pressed="true"
		android:color="?attr/colorOnSecondaryContainerStateContent" />
	<item
		android:state_focused="true"
		android:state_checked="true"
		android:color="?attr/colorOnSecondaryContainerStateContent" />
	<item
		android:state_checked="true"
		android:color="?attr/colorOnSecondaryContainerStateContent"
		android:state_hovered="true" />
	<item
		android:state_checked="true"
		android:color="?attr/colorOnSecondaryContainer" />
	<item
		android:state_pressed="true"
		android:color="?attr/colorOnSurfaceStateContent" />
	<item
		android:state_focused="true"
		android:color="?attr/colorOnSurfaceStateContent" />
	<item
		android:color="?attr/colorOnSurfaceStateContent"
		android:state_hovered="true" />
	<item
		android:color="?attr/colorOnSurface" />
</selector>
